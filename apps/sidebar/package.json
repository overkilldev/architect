{
  "name": "@architect/sidebar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "yarn start",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vite build",
    "compile": "echo skip",
    "postbuild": "yarn run write-style-path-to-manifest",
    "write-style-path-to-manifest": "sh stylesToManifest.sh",
    "preview": "vite preview",
    "lint": "eslint --ignore-path ../../.gitignore --ignore-pattern !cypress/.eslintrc.js --max-warnings=0 --ext .ts,.tsx .",
    "test:ci": "cross-env CI=true jest --runInBand",
    "test:all": "vitest run --coverage",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path ../../.gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different"
  },
  "dependencies": {},
  "devDependencies": {
    "@architect/types": "*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
