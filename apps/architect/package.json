{
  "name": "architect",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "yarn start",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path ../../.gitignore --ignore-pattern !cypress/.eslintrc.js --ext .ts,.tsx .",
    "test:ci": "cross-env CI=true jest --runInBand",
    "test:all": "vitest run --coverage",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "elkjs": "^0.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-flow-renderer": "^10.3.5",
    "react-query": "^3.39.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "web-worker": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/faker": "5.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.0",
    "faker": "5.5.3",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "turbo": "^1.2.16"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
